{
  "testMatch": [
    "**/?(*.)+(spec|test).[jt]s?(x)"
  ],
  "coverageDirectory": "reports/junit/jest/coverage",
  "collectCoverageFrom": [
    "**/src/**/*.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "restoreMocks": true,
  "clearMocks": true,
  "resetMocks": true
}
